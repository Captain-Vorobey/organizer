<p id="notice"><%= notice %></p>

<h1>Time Limits</h1>

<table>
  <thead>
    <tr>
      <th>Start time</th>
      <th>End time</th>
      <th>User</th>
      <th>Service</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_limits.each do |time_limit| %>
      <tr>
        <td><%= time_limit.start_time %></td>
        <td><%= time_limit.end_time %></td>
        <td><%= time_limit.user_id %></td>
        <td><%= time_limit.service_id %></td>
        <td><%= link_to 'Show', time_limit %></td>
        <td><%= link_to 'Edit', edit_time_limit_path(time_limit) %></td>
        <td><%= link_to 'Destroy', time_limit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time Limit', new_time_limit_path %>
